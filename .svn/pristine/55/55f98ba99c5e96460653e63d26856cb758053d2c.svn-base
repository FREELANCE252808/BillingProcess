@keyframes pageLoad {
	from {
		opacity: 1;
	}

	to {
		opacity: 0;
	}
}

.fury-page-load-animation {
	opacity: 1;
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
	animation: pageLoad ease-in-out 1;
	animation-fill-mode: forwards;
	animation-duration: 400ms;
	animation-delay: 600ms;
	background: $sidenav-background;
	z-index: 9999;
	pointer-events: none;
}

