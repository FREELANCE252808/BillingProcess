import { BaseModel } from './_base.model';

export class EmailsModel extends BaseModel {
	id: number;
	emailDesc: string;
	emailSubject: string;
	emailMessageBody: string;
	smsMessage: string;
	createdOn: string;
	createdBy: string;
	modifiedOn: string;
	modifiedBy: string;
	companyID: string;
	status: string;
	emailSMSTemplateID: number;


	clear() {
		this.emailDesc = '';
		this.emailSubject = '';
		this.emailMessageBody = '';
		this.smsMessage = '';
		this.createdOn = '';
		this.createdBy = '';
		this.modifiedOn = '';
		this.modifiedBy = '';
		this.companyID = '';
		this.status = 'A';
		this.emailSMSTemplateID = 0;

	}
}

